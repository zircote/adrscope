{
  "problemMatcher": [
    {
      "owner": "adrscope",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^ERROR:\\s+([^:]+)(?::(\\d+))?\\s+-\\s+(.+?)\\s+\\[([^\\]]+)\\]$",
          "file": 1,
          "line": 2,
          "message": 3,
          "code": 4
        }
      ]
    },
    {
      "owner": "adrscope",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^WARNING:\\s+([^:]+)(?::(\\d+))?\\s+-\\s+(.+?)\\s+\\[([^\\]]+)\\]$",
          "file": 1,
          "line": 2,
          "message": 3,
          "code": 4
        }
      ]
    }
  ]
}
